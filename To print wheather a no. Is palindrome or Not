#include <stdio.h>
int main() {
    int n, i, flag=0;
    printf("Enter the no:");
    scanf ("%d",&n);
    if(n==0||n==1){
       int flag=1;
    }for(i=2;i<=n/2;i++){
        if(n%i==0){
            flag=1;
            break;
        }
    }
        if(flag==0){
          printf ("no. is palindrome");
        }else
       printf("no.is not palindrome") ;
    
            return 0;
}

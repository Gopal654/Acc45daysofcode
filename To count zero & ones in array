
#include <stdio.h>
int main() {
    int n, countzero=0, countOne=0;
    printf("enter the size of array:");
    scanf("%d",&n);
    
    int a[n];
    printf ("enter the elements:");
    
    for(int i=0;i<n;i++){
       scanf("%d",&a[i]);
       if(a[i]==0){
        countzero=countzero+1;
        }else if (a[i]==1){
           countOne=countOne+1;
       }
    }
 printf ("zero's are:%d\n",countzero);
printf ("ones are:%d\n",countOne);

    return 0;
}

#include <stdio.h>
int main() {
    int n, i, a[n],sum=0;
    printf("enter the size:");
    scanf("%d",&n);
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
        if(a[i]<0){
            sum=sum+a[i];
        }
        else{
            printf ("not a valid no.");
        }
    }
    printf ("sum is:%d",sum);
 return 0;
}
